{
  "expo": {
    "name": "Giper",
    "slug": "poizonG",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Group59.png",
    "backgroundColor": "#fff",
    "scheme": "poizong",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#fff",
      "bundleIdentifier": "com.shukurhm.poizonG"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.shukurhm.poizonG"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#5600B3",
          "image": "./assets/images/giper1.png",
          "dark": {
            "image": "./assets/images/giper1.png",
            "backgroundColor": "#5600B3"
          },
          "imageWidth": 100
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
